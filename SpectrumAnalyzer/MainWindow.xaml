<?xml version="1.0" encoding="utf-8"?>
<Controls:MetroWindow
	x:Class="SpectrumAnalyzer.MainWindow" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:Controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
	Title="Spectrum Analyzer"
	Height="600"
	Width="800"
	Background="DarkGray">
	<Grid
		x:Name="MainWindowGrid"
		RenderTransformOrigin="0.5,0.5">
		<Grid.RowDefinitions>
			<RowDefinition />
			<RowDefinition />
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition
				Width="0.499999999999999*" />
		</Grid.ColumnDefinitions>
		<Ellipse
			Fill="#00FFFFFF"
			Stroke="#FF000000"
			StrokeThickness="2"
			Width="144"
			Height="138"
			Grid.Column="0"
			Grid.Row="0"
			HorizontalAlignment="Left"
			VerticalAlignment="Top"
			Margin="53,59,0,0" />
	</Grid>
	<Controls:MetroWindow.RightWindowCommands>
		<Controls:WindowCommands>
			<Button
				Content="Save Settings" />
		</Controls:WindowCommands>
	</Controls:MetroWindow.RightWindowCommands>
</Controls:MetroWindow>